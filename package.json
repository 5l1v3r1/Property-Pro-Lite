{
  "name": "property-pro-lite-server",
  "version": "1.0.0",
  "description": "Property Pro Lite",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel server/ -d ./build/ -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/index.js",
    "dev": "nodemon server/index.js --exec babel-node",
    "lint": "eslint ./ --fix",
    "test": "nyc mocha ./server/tests/* --require @babel/register --require @babel/polyfill --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "property",
    "property lease",
    "sell house",
    "buy house",
    "rent house",
    "house agents",
    "hotels",
    "houses in Naija"
  ],
  "author": "Fortune King",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/chunkingz/Property-Pro-Lite.git"
  },
  "bugs": {
    "url": "https://github.com/chunkingz/Property-Pro-Lite/issues"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^4.0.1",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.6",
    "cloudinary": "^1.14.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.0",
    "form-data": "^2.5.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
